{
  "name": "@adobe/helix-rum-collector",
  "version": "1.2.1",
  "private": true,
  "description": "Collect Real User Monitoring Data for Helix",
  "main": "src/index.js",
  "scripts": {
    "prebuild": "webpack",
    "build": "ecp-js",
    "deploy": "npm run build && fastly compute deploy -t $HLX_FASTLY_AUTH",
    "test": "mocha test/unit/*.test.js",
    "lint": "./node_modules/.bin/eslint .",
    "semantic-release": "semantic-release",
    "asbuild:untouched": "asc assembly/index.ts --target debug",
    "asbuild:optimized": "asc assembly/index.ts --target release",
    "astest": "asp",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "cross-platform-bundle": "mkdir -p pkg;tar --version | grep bsdtar && tar  zcvf pkg/helix3.tar.gz -s ';^;helix3/;' bin fastly.toml package.json || tar  zcvf pkg/helix3.tar.gz --transform 's,^,helix3/,' --show-transformed-names  bin fastly.toml package.json",
    "bundle": "npm run asbuild:optimized && npm run cross-platform-bundle",
    "fastly-deploy": "fastly compute deploy",
    "test-postdeploy": "mocha test/js --reporter xunit --reporter-options output=./junit/test-results.xml -g 'Post-Deploy'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-rum-collector.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-rum-collector/issues"
  },
  "homepage": "https://github.com/adobe/helix-rum-collector#readme",
  "dependencies": {
    "@fastly/js-compute": "file:vendor/fastly-js-compute-0.0.1.tgz"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.1.5",
    "@as-pect/cli": "6.1.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "chai": "4.3.4",
    "chai-http": "4.3.0",
    "eslint": "7.28.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.23.4",
    "jsdoc-to-markdown": "7.0.1",
    "mocha": "8.4.0",
    "nyc": "15.1.0",
    "semantic-release": "17.4.3",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  }
}
