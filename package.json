{
  "name": "@adobe/helix-rum-collector",
  "version": "1.16.1",
  "private": true,
  "description": "Collect Real User Monitoring Data for Helix",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack && js-compute-runtime bin/index.js bin/main.wasm",
    "fastly-build": "fastly compute build --verbose",
    "deploy": "npm run fastly-build && fastly compute deploy -t $HLX_FASTLY_AUTH",
    "test": "nyc --reporter=text --reporter=lcov -i -g 'Post-Deploy' --check-coverage --branches 95 --statements 100 --lines 100 mocha -i -g 'Post-Deploy'",
    "test-ci": "nyc --reporter=text --reporter=lcov -i -g 'Post-Deploy' --check-coverage --branches 95 --statements 100 --lines 100 mocha -i -g 'Post-Deploy' --reporter xunit --reporter-options output=./junit/test-results.xml --exit && codecov",
    "test-postdeploy": "mocha --reporter xunit --reporter-options output=./junit/test-results.xml -g 'Post-Deploy'",
    "lint": "./node_modules/.bin/eslint .",
    "semantic-release": "semantic-release",
    "fastly-deploy": "fastly compute deploy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-rum-collector.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-rum-collector/issues"
  },
  "homepage": "https://github.com/adobe/helix-rum-collector#readme",
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.3.2",
    "@adobe/semantic-release-coralogix": "1.1.11",
    "@fastly/js-compute": "0.5.13",
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "chai": "4.3.7",
    "chai-http": "4.3.0",
    "codecov": "3.8.3",
    "eslint": "8.29.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.26.0",
    "jsdoc-to-markdown": "8.0.0",
    "mocha": "10.1.0",
    "mocha-junit-reporter": "2.2.0",
    "nyc": "15.1.0",
    "semantic-release": "19.0.5",
    "semantic-release-slack-bot": "3.5.3",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.0"
  }
}
